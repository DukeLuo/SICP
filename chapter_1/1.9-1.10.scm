#lang sicp

;;; recursion
(define (factorial n)
  (if (= n 1)
      1
      (* n (factorial (- n 1)))))


;;; iteration
(define (factorial n)
  (define (iter product counter)
  (if (> counter n)
      product
      (iter (* counter product)
            (+ counter 1))))
  (iter 1 1))
  
 
